{
  "name": "streamtv",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "create": "npm run build && npm run test",
    "build": "tsc -p .",
    "test": "jest --coverage",
    "watch": "jest --watch",
    "refresh": "rm -rf ./node_modules ./package-lock.json && npm install",
    "clean": "find ./var/cache -type f ! -name \"*.*\" -exec rm -rf {} \\;",
    "encrypt": "npm run clean && node lib/src/index.js -vy $STREAM_TV_APP_HOSTS_CONFIG | ./node_modules/pino-pretty/bin.js -t \"yyyy-mm-dd HH:MM:ss\" -c -i \"pid,hostname\"",
    "news": "node lib/src/index.js -vy $STREAM_TV_APP_NEWS_CONFIG | ./node_modules/pino-pretty/bin.js -t \"yyyy-mm-dd HH:MM:ss\" -c -i \"pid,hostname\"",
    "shows": "node lib/src/index.js -vy $STREAM_TV_APP_SHOWS_CONFIG | ./node_modules/pino-pretty/bin.js -t \"yyyy-mm-dd HH:MM:ss\" -c -i \"pid,hostname\""
  },
  "main": "./lib/index.js",
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/bluebird": "^3.5.26",
    "@types/cheerio": "^0.22.11",
    "@types/crypto-js": "^3.1.43",
    "@types/glob": "^7.1.1",
    "@types/js-yaml": "^3.12.1",
    "@types/node-fetch": "^2.3.4",
    "@types/pino": "^5.8.7",
    "@types/underscore": "^1.8.18",
    "bluebird": "^3.5.4",
    "chalk": "^2.4.2",
    "cheerio": "^1.0.0-rc.2",
    "clear": "^0.1.0",
    "commander": "^2.20.0",
    "crypto-js": "^3.1.9-1",
    "dotenv": "^8.0.0",
    "figlet": "^1.2.1",
    "get-video-duration": "^1.0.3",
    "glob": "^7.1.3",
    "inversify": "^5.0.1",
    "inversify-inject-decorators": "^3.1.0",
    "js-yaml": "^3.13.1",
    "node-fetch": "^2.3.0",
    "node-fetch-retry": "^1.0.1",
    "path": "^0.12.7",
    "pino": "^5.12.5",
    "pino-pretty": "^3.0.0",
    "reflect-metadata": "^0.1.13",
    "underscore": "^1.9.1",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/jasmine": "^3.3.12",
    "@types/jest": "^24.0.13",
    "@types/node": "^11.13.11",
    "babel-jest": "^24.8.0",
    "babel-preset-trigen": "^2.2.1",
    "jest": "^24.8.0",
    "jest-each": "^24.8.0",
    "jest-fetch-mock": "^2.1.2",
    "nodemon": "^1.18.10",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.0.3",
    "typescript": "^3.4.1"
  }
}
